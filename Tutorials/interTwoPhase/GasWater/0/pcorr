FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      pcorr;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    sides
    {
        type            fixedValue;
        value           uniform 0;
    }
    empty1
    {
        type            empty;
    }
    empty2
    {
        type            empty;
    }
}


